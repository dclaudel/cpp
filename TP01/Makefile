all: Exos1 Exos2 Exos3 Exos4 Exos5 Exos6 clean

# apres l'etiquete (test:) un .o pour chaque .cpp compil√©
# cette ligne genere un executable appele 'test'
Exos1: Exos1.cpp
	g++ -Wall Exos1.cpp -o Exos1

Exos2: Exos2.cpp
	g++ -Wall Exos2.cpp -o Exos2

Exos3: Exos3.cpp
	g++ -Wall Exos3.cpp -o Exos3

Exos4: Exos4.cpp
	g++ -Wall Exos4.cpp -o Exos4

Exos5: Exos5.cpp
	g++ -Wall Exos5.cpp -o Exos5

Exos6: Exos6.cpp
	g++ -Wall Exos6.cpp -o Exos6

clean:
	rm -f *.o test
